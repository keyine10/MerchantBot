{
  "name": "merchantbot",
  "version": "0.0.1",
  "description": "discord bot for searching on mercarijp",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && npx tsc",
    "start": "npm run redeploy-commands && node dist/index.js | npx pino-pretty -i pid,hostname -t 'yyyy-mm-dd HH:MM:ss'",
    "deploy": "npm run redeploy-commands && pm2 reload dist/index.js --name merchant-bot --update-env || pm2 start dist/index.js --name merchant-bot && pm2 save",
    "redeploy-commands": "npm run build && node dist/deploy-commands.js",
    "test": "npx jest mercari/mercari.test.ts",
    "logs": "TZ=Asia/Bangkok npx pino-pretty -i pid,hostname -T 'yyyy-mm-dd HH:MM:ss.l o' <"
  },
  "keywords": [
    "discord",
    "bot",
    "mercari"
  ],
  "author": "celen",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.29",
    "@types/sharp": "^0.31.1",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "eslint": "^9.22.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@types/mongoose": "^5.11.96",
    "@types/node-cron": "^3.0.11",
    "bottleneck": "^2.19.5",
    "dayjs": "^1.11.13",
    "discord.js": "^14.19.3",
    "dotenv": "^16.4.7",
    "jest": "^29.7.0",
    "jose": "^6.0.10",
    "mongoose": "^8.15.1",
    "node-cron": "^4.1.0",
    "pino": "^7.0.0-rc.9",
    "pino-pretty": "^13.0.0",
    "sharp": "^0.34.2",
    "tsconfig": "^7.0.0",
    "uuid": "^11.1.0"
  }
}
